  height: 1.5em;
}

.Tag {
  display: inline-block;
  font-size: 0.6em;
  height: 1.5em;
  background: rgb(200, 25, 25);
  color: rgba(255, 255, 255, 0.9);
  padding: 2px;
  margin: 2px;
  padding-left: 4px;
  padding-right: 4px;
  border-radius: 4px;
  font-style: normal;
  /* vertical-align: middle; */
}
body.modal-open {
  overflow: hidden;
}

.Modal {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.35);
  z-index: 1000;
}

.Modal .modal-container {
  padding: 1em;
  padding-top: 0px;
  padding-bottom: 0px;
  background: white;
  overflow: auto;
  min-height: 4em;
}

@media only screen and (max-width: 768px) {
  .Modal .modal-container {
    height: 100%;
    padding: 2em;
    padding-top: 0;
    padding-bottom: 0;
  }
}

@media only screen and (min-width: 768px) {
  @keyframes modal-slide-in {
    from {
      top: 0.2em;
      opacity: 0;
    }

    to {
      top: 1em;
      opacity: 1;
    }
  }

  .Modal .modal-container {
    max-width: 700px;
    position: absolute;
    top: 1em;
    /* animation-duration: 0.5s;
    animation-name: modal-slide-in; */
    max-height: calc(100vh - 2em);
    left: 0;
    right: 0;
    margin-left: auto;
    margin-right: auto;
    -webkit-box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.4);
    -moz-box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.4);
    box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.4);
    border-radius: 4px;
  }
}

.Modal .modal-close-btn {
  float: right;
  position: relative;
  top: 1em;
  width: 1.5em;
  height: 1.5em;
  opacity: 0.3;
}
.Modal .modal-close-btn:hover {
  opacity: 1;
}
.Modal .modal-close-btn:before,
.Modal .modal-close-btn:after {
  position: absolute;
  left: 50%;
  content: ' ';
  height: 100%;
  width: 2px;
  background-color: #333;
}
.Modal .modal-close-btn:before {
  transform: rotate(45deg);
}
.Modal .modal-close-btn:after {
  transform: rotate(-45deg);
}

.SpellTable tbody tr:hover {
  color: white;
  background: #009ba1;
}

.SpellTable tbody tr.active {
  color: white;
  background: #006064;
}

.SpellTable {
  margin-top: 10px;
  width: 100%;
}

.SpellTable .spell-col {
  display: flex;
}

.SpellTable .spell-name {
  flex-grow: 1;
  text-align: left;
}

.Select {
  margin: 2px;
  border: none;
  border-bottom: 2px solid #dee4eb;
  padding: 5px;
  padding-bottom: 2px;
  position: relative;
  width: 180px;
  height: calc(1em + 5px);
  display: inline-block;
}

.Select .container {
  display: flex;
}

.Select:focus {
  outline: none;
  border-color: #006064;
}

.Select .menu {
  position: absolute;
  top: 38px;
  left: 2px;
  width: 100%;
  padding-top: 4px;
  padding-bottom: 4px;
  -webkit-box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.4);
  -moz-box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.4);
  box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.4);
  border-radius: 4px;
  background: white;
  outline: none;
  z-index: 500;
  max-height: 200px;
  overflow-y: auto;
  /* overflow: hidden; */
  /* border: 1px solid #ccc; */
}

.Select .menu .option {
  padding-left: 0.5em;
  padding-right: 0.5em;
  padding-top: 0.25em;
  padding-bottom: 0.25em;
  user-select: none;
}

.Select .menu .option:focus {
  outline: none;
}

.Select .menu .option:hover {
  background: #009ba1;
  color: white;
}

.Select .menu .selected {
  background: #006064 !important;
  color: white;
}

.Select .label {
  flex-grow: 1;
  font-size: 0.9em;
  vertical-align: baseline;
}

.arrow {
  border: solid;
  border-width: 0 2px 2px 0;
  display: inline-block;
  padding: 3px;
  position: absolute;
  top: 11px;
  right: 5px;
}

.right {
  transform: rotate(-45deg);
  -webkit-transform: rotate(-45deg);
}

.left {
  transform: rotate(135deg);
  -webkit-transform: rotate(135deg);
}

.up {
  border-color: #006064;
  transform: rotate(-135deg);
  -webkit-transform: rotate(-135deg);
}

.Select:hover .down {
  border-color: #006064 !important;
}

.down {
  border-color: #dee4eb;
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
}

.Label {
  color: #999;
  font-weight: bold;
  margin: auto;
}

