img {
  display: block;
  margin-left: auto;
  margin-right: auto;
  border: 2px solid #dee4eb;
  border-radius: 3px;
  height: 300px;
}

.flex {
  display: flex;
}

.fill {
  flex-grow: 1;
}

.input {
  display: flex;
}

.input .search-name {
  flex-grow: 1;
  margin-right: 10px;
}
.UrlLoader {
  margin: auto;
  width: 200px;
  padding: 15px;
}

.UrlLoader .url-fields {
  padding-bottom: 5px;
  width: 100%;
}

.UrlLoader .url-fields input {
